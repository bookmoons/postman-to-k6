{
    "name": "postman-to-k6",
    "description": "Convert a Postman collection to k6 JavaScript",
    "version": "0.1.2",
    "license": "Apache-2.0",
    "keywords": [
        "k6",
        "postman",
        "loadtest",
        "load-test",
        "load-testing",
        "server-load",
        "performance"
    ],
    "homepage": "https://github.com/loadimpact/postman-to-k6",
    "bugs": "https://github.com/loadimpact/postman-to-k6/issues",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/loadimpact/postman-to-k6.git"
    },
    "engines": {
        "node": ">= 0.10.0"
    },
    "preferGlobal": "true",
    "directories": {
        "bin": "bin",
        "lib": "lib"
    },
    "bin": {
        "postman-to-k6": "bin/postman-to-k6.js"
    },
    "dependencies": {
        "ajv": "^6.10.0",
        "aws4": "^1.8.0",
        "browserify": "^16.2.3",
        "chai": "^4.2.0",
        "cheerio": "^1.0.0-rc.2",
        "commander": "^2.19.0",
        "crypto-js": "^3.1.9-1",
        "filenamify": "^3.0.0",
        "fs-extra": "^7.0.1",
        "lodash": "^4.17.11",
        "oauth-1.0a": "^2.2.5",
        "papaparse": "^4.6.3",
        "pkginfo": "^0.4.1",
        "postman-collection": "^3.4.3",
        "postman-collection-transformer": "^3.0.0",
        "prettier": "^1.16.4",
        "strip-json-comments": "^2.0.1",
        "urijs": "^1.19.1",
        "uuid": "^3.3.2",
        "verror": "^1.10.0",
        "xml2js": "^0.4.19"
    },
    "devDependencies": {
        "@babel/plugin-transform-runtime": "^7.4.0",
        "@babel/preset-env": "^7.3.4",
        "@babel/register": "^7.0.0",
        "@babel/runtime": "^7.4.2",
        "ava": "^1.2.1",
        "cross-env": "^5.2.0",
        "mock-require": "^3.0.3",
        "npm-run-all": "^4.1.5",
        "sinon": "^7.2.7",
        "snazzy": "^8.0.0",
        "standard": "^12.0.1"
    },
    "scripts": {
        "bundle": "node scripts/bundle.js",
        "lint-js": "standard --verbose | snazzy",
        "lint": "npm-run-all lint-js",
        "postinstall": "node scripts/bundle.js",
        "test-com": "cross-env NODE_PATH=lib:test ava test/com",
        "test-int": "cross-env NODE_PATH=lib:test ava test/int",
        "test-unit": "cross-env NODE_PATH=lib:test ava test/unit",
        "test": "npm-run-all test-unit test-com test-int",
        "test-hoc": "cross-env NODE_PATH=lib:test ava"
    }
}
